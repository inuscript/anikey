{
  "name": "keyshond",
  "version": "0.0.1",
  "description": "CSS in JS friendly animation helper for convert `Element.animate()` object",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "budo docs/index.js:index.js --dirs docs -H localhost",
    "prebuild": "rimraf dist",
    "build": "babel lib --out-dir dist",
    "preversion": "npm run build",
    "test": "mocha test/"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "keywords": [
    "css",
    "CSS in JS",
    "animation",
    "animations",
    "keyframe",
    "keyframes",
    "inline",
    "style",
    "inline-styless"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "aphrodite": "^0.5.0",
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "budo": "^9.0.0",
    "enroute": "^1.0.1",
    "free-style": "^1.2.2",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "redom": "0.0.5"
  }
}
