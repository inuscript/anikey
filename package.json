{
  "name": "keyshond",
  "version": "0.0.5",
  "description": "CSS in JS friendly animation helper support Element.animate() object and CSS @keyframes",
  "main": "lib/index.js",
  "modules": "src/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "babel src --out-dir lib",
    "preversion": "npm run build",
    "postversion": "git push --tag",
    "lint": "eslint src/ test/",
    "test": "npm run build; mocha test/",
    "coverage": "nyc npm test",
    "report-coverage": "nyc --reporter=lcov report > coverage.lcov"
  },
  "files": [
    "lib",
    "src"
  ],
  "keywords": [
    "css",
    "CSS in JS",
    "animation",
    "animations",
    "keyframe",
    "keyframes",
    "inline",
    "style",
    "inline-styless"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inuscript/keyshond.git"
  },
  "author": "inuscript",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-latest": "^6.14.0",
    "eslint": "^3.4.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "mocha": "^3.0.2",
    "nyc": "^8.1.0",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "hyphenate-style-name": "^1.0.1",
    "keyframe-transpose": "^1.0.0"
  }
}
